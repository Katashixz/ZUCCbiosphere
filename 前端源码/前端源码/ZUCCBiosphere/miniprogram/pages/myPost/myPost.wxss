.contain .mySinglePost {
  margin: 20rpx;
  border-radius: 20rpx;
  border-bottom: 2rpx #c0bebe solid;
  display: flex;
}
.contain .mySinglePost .myPostContent {
  display: flex;
  height: 150rpx;
  width: 600rpx;
  font-size: 35rpx;
  font-weight: bold;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.contain .mySinglePost .myPostContent .postType {
  color: dodgerblue;
}
.contain .mySinglePost .postImage {
  border-radius: 20rpx;
  margin: 20rpx;
  height: 200rpx;
  width: 200rpx;
}
